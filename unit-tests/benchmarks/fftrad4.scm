;; LC MACROS & UTILS

(##define-macro (def-macro form . body)
  `(##define-macro ,form (let () ,@body)))

(def-macro (FLOATvector-const . lst) `',(list->vector lst))
(def-macro (FLOATvector-ref v i)     `(vector-ref ,v ,i))
(def-macro (FLOATvector-set! v i n)  `(vector-set! ,v ,i ,n))
(def-macro (FLOATmake-vector . l)    `(make-vector ,@l))
(def-macro (FLOATvector-length v)    `(vector-length ,v))
(def-macro (FLOATsin v)    `(sin ,v))
(def-macro (FLOATvector . lst) `(vector ,@lst))

(def-macro (arithmetic-shift n s) `(lcshift ,n ,s))
(define (lcshift n s)
  (cond ((= s 0) n)
        ((> s 0) (* n (expt 2 s)))
        (else
          (if (and (< n 0) (odd? n))
              (error "NYI case shr")
              (quotient n (expt 2 (* -1 s)))))))

